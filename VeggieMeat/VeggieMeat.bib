 issn = {20726643},
 journal = {Nutrients},
 doi = {10.3390/nu11112603},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6893642},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/31671655},
