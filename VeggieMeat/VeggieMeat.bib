 author = {Bianchi, F. and Aveyard, P. and Astbury, N. M. and Cook, B. and Cartwright, E. and Jebb, S. A.},
 year = {2019},
 title = {Replacing meat with alternative plant-based products (RE-MAPs): Protocol for a randomised controlled trial of a behavioural intervention to reduce meat consumption},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85066765198&doi=10.1136%2fbmjopen-2018-027016&partnerID=40&md5=0f388fca8c514003c5841242d58e1b86},
 keywords = {adverse event;Article;behavior therapy;biological marker;blood pressure;body composition;body weight;controlled study;Diet Records;diet therapy;feeding behavior;follow up;food intake;health hazard;human;Humans;interview;lipid;lipid blood level;meat;medical record;outcome assessment;plant product;procedures;psychology;qualitative research;randomized controlled trial;randomized controlled trial (topic);Randomized Controlled Trials as Topic;single blind procedure;Single-Blind Method;statistical model},
 volume = {9},
 number = {5},
 issn = {20446055},
 journal = {BMJ Open},
