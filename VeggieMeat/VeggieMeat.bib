}


@article{Comi.2016,
 abstract = {Cooked bacon is a typical Italian meat product. After production, cooked bacon is stored at 4~$\pm$~2~°C. During storage, the microorganisms that survived pasteurisation can grow and produce spoilage. For the first time, we studied the cause of the deterioration in spoiled cooked bacon compared to unspoiled samples. Moreover, the use of bio-protective cultures to improve the quality of the product and eliminate the risk of spoilage was tested. The results show that Leuconostoc mesenteroides is responsible for spoilage and produces a greening colour of the meat, slime and various compounds that result from the fermentation of sugars and the degradation of nitrogen compounds. Finally, Lactococcus lactis spp. lactis and Lactobacillus sakei were able to reduce the risk of Leuconostoc mesenteroides spoilage.},
 author = {Comi, Giuseppe and Andyanto, Debbie and Manzano, Marisa and Iacumin, Lucilla},
 year = {2016},
 title = {Lactococcus lactis and Lactobacillus sakei as bio-protective culture to eliminate Leuconostoc mesenteroides spoilage and improve the shelf life and sensorial characteristics of commercial cooked bacon},
 pages = {16--22},
 volume = {58},
 journal = {Food microbiology},
 doi = {10.1016/j.fm.2016.03.001},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/27217354}
}


